{
  "name": "friendsoftypo3headless/headless-kesearch",
  "type": "typo3-cms-extension",
  "description": "This extension provides integration with ke_search to output content from TYPO3 in JSON format.",
  "version": "1.0.0",
  "keywords": [
    "TYPO3",
    "headless",
    "pwa",
    "json",
    "ke_search"
  ],
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Sven Petersen",
      "role": "Developer",
      "email": "sven@hardanders.de"
    },
    {
      "name": "Niels Seelh√∂fer",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5"
  },
  "require-dev": {
    "helmich/typo3-typoscript-lint": "^2.5"
  },
  "suggest": {
    "tpwd/ke_search": "^4.2",
    "friendsoftypo3/headless": "^2.6 || ^3.0"
  },
  "extra": {
     "typo3/cms": {
        "extension-key": "headless_kesearch"
     }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
