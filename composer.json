{
  "name": "friendsoftypo3headless/headless-kesearch",
  "type": "typo3-cms-extension",
  "description": "This extension provides integration with ke_search to output content from TYPO3 in JSON format.",
  "keywords": [
    "TYPO3",
    "headless",
    "pwa",
    "json",
    "ke_search"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "typo3/cms-core": "^10.4 || ^11.5",
    "tpwd/ke_search": "^4.2"
  },
  "require-dev": {
    "helmich/typo3-typoscript-lint": "^2.5",
    "symplify/easy-coding-standard": "^10.0",
    "phpstan/phpstan": "^1.3",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "symfony/var-dumper": "^6.0",
    "thecodingmachine/phpstan-strict-rules": "^1.0",
    "saschaegerer/phpstan-typo3": "^1.0",
    "phpstan/extension-installer": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Friendsoftypo3headless\\HeadlessKesearch\\": "Classes"
    }
  },
  "suggest": {
    "friendsoftypo3/headless": "^3.0"
  },
  "extra": {
     "typo3/cms": {
        "extension-key": "headless_kesearch"
     }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true,
      "phpstan/extension-installer": true
    }
  }
}
